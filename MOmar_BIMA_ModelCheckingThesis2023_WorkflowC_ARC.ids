        <copyright>MohamedOmar_BIMAPlusThesis2023</copyright>
        <version>V1</version>
        <description>As part of BIM A+ Thesis Work, this IDS consists of  quality, and information requirements checks of Architectural Models that can be checked aganist IFC project files in any Open BIM software that supports IDS.</description>
        <author>mohamedahmedomar807@gmail.com</author>
        <date>2023-08-04</date>
        <milestone>PZI</milestone>
    </info>
    <specifications>
        <specification name="AR.1.a Ceilings-TypeName" ifcVersion="IFC4" description="PASS/FAIL check to determine if any Ceilings' Type Names are not following the Company Naming Convention . Will Fail if any are found." minOccurs="0" maxOccurs="unbounded">
            <applicability>
                <entity>
                    <name>
                        <simpleValue>IFCCOVERING</simpleValue>
                    </name>
                    <predefinedType>
                        <simpleValue>CEILING</simpleValue>
                    </predefinedType>
                </entity>
            </applicability>
            <requirements>
                <property measure="IfcText" minOccurs="1" maxOccurs="unbounded">
                    <propertySet>
                        <simpleValue>BOS_General</simpleValue>
                    </propertySet>
                    <name>
                        <simpleValue>Type</simpleValue>
                    </name>
                    <value>
                        <xs:restriction base="xs:string">
                            <xs:pattern value="RP-AR-(STCL|SSCL)-(GB|MS|WD|PS|TI|PI)(_MDF|_ACS|_AQU|_XPS|_EPS|_MNW|_WTR)??-[0-9]*mm" />
                        </xs:restriction>
                    </value>
                </property>
            </requirements>
        </specification>
        <specification name="AR.1.b Ceilings-Workset" ifcVersion="IFC4" description="PASS/FAIL check to determine if any Ceilings are not modeled correctly in Structural Workset. Will Fail if any are found in wrong workset." minOccurs="0" maxOccurs="unbounded">
            <applicability>
                <entity>
                    <name>
                        <simpleValue>IFCCOVERING</simpleValue>
                    </name>
                    <predefinedType>
                        <simpleValue>CEILING</simpleValue>
                    </predefinedType>

            <requirements>
                <property measure="IfcText" minOccurs="1" maxOccurs="unbounded">
                    <propertySet>
                        <simpleValue>BOS_General</simpleValue>
                    </propertySet>
                    <name>
                        <simpleValue>Workset</simpleValue>
                    </name>
                    <value>
                        <xs:restriction base="xs:string">
                            <xs:pattern value="RP-AR-(Facade|Interior)" />
                        </xs:restriction>
                    </value>
                </property>
            </requirements>
        </specification>
        <specification name="AR.1.c Ceilings-TypeMark" ifcVersion="IFC4" description="PASS/FAIL check to determine if any Ceilings' Type Marks are not following the Company Naming Convention . Will Fail if any are found." minOccurs="0" maxOccurs="unbounded">
